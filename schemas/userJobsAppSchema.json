{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "title": "Root Schema",
  "type": "object",
  "default": {},
  "required": ["username", "job_id"],
  "properties": {
    "username": {
      "title": "The username Schema",
      "type": "string",
      "default": "",
      "examples": ["bob"]
    },
    "job_id": {
      "title": "The job_id Schema",
      "type": "integer",
      "default": 0,
      "examples": [24]
    }
  },
  "examples": [
    {
      "username": "bob",
      "job_id": 24
    }
  ]
}
